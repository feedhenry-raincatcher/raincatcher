{
  "name": "@raincatcher/logger",
  "version": "1.1.2",
  "description": "Logger facade used by RainCatcher modules",
  "types": "src/index.ts",
  "author": "feedhenry-raincatcher@redhat.com",
  "license": "MIT",
  "main": "src/",
  "scripts": {
    "clean": "del src/**/*.js src/**/*.map test/**/*.js test/**/*.map",
    "build": "tsc",
    "test": "npm run clean && nyc mocha",
    "example": "ts-node example/index.ts"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "report-dir": "../../coverage_report/client-cloud-logger",
    "check-coverage": true,
    "lines": 75,
    "functions": 100,
    "branches": 80
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/chai": "4.1.3",
    "@types/mocha": "5.2.0",
    "chai": "4.1.2",
    "del-cli": "1.1.0",
    "mocha": "5.1.1",
    "nyc": "11.7.1",
    "source-map-support": "0.5.5",
    "ts-node": "6.0.2",
    "typescript": "2.8.3"
  },
  "dependencies": {
    "@types/bunyan": "1.8.4",
    "@types/loglevel": "1.5.3",
    "bunyan": "1.8.12",
    "loglevel": "1.6.1"
  }
}
