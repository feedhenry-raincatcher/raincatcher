{
  "name": "@raincatcher/datasync-client",
  "version": "1.1.2",
  "description": "Raincatcher datasync client wrapper for TypeScript",
  "types": "src/index.ts",
  "main": "src/",
  "author": "feedhenry-raincatcher@redhat.com",
  "license": "MIT",
  "scripts": {
    "clean": "del src/**/*.js src/**/*.map test/**/*.js test/**/*.map",
    "build": "tsc",
    "test": "nyc mocha"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "report-dir": "../../coverage_report/client-datasync-client",
    "check-coverage": true,
    "lines": 75,
    "functions": 60,
    "branches": 45
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/chai": "4.1.2",
    "@types/mocha": "2.2.48",
    "@types/proxyquire": "1.3.28",
    "@types/sinon": "4.1.3",
    "browserify-shim": "3.8.14",
    "chai": "4.1.2",
    "del-cli": "1.1.0",
    "mocha": "5.0.1",
    "nyc": "11.4.1",
    "proxyquire": "1.8.0",
    "sinon": "4.3.0",
    "source-map-support": "0.5.3",
    "ts-node": "4.1.0",
    "typescript": "2.7.2"
  },
  "dependencies": {
    "@raincatcher/logger": "1.1.2",
    "@types/bluebird": "3.5.20",
    "@types/lodash": "4.14.104",
    "bluebird": "3.5.1",
    "fh-sync-js": "1.3.0",
    "lodash": "4.17.5"
  }
}
