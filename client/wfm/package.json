{
  "name": "@raincatcher/wfm",
  "version": "1.1.2",
  "description": "Set of WorkForce Management tools",
  "types": "src/index.ts",
  "author": "feedhenry-raincatcher@redhat.com",
  "license": "MIT",
  "main": "src/",
  "scripts": {
    "clean": "del src/*.js src/*.map src/**/*.js src/**/*.map test/*.js test/*.map test/**/*.js test/**/*.map",
    "build": "tsc",
    "test": "nyc mocha",
    "debug": "node --inspect-brk ./node_modules/.bin/_mocha",
    "debug-legacy": "node --debug-brk ./node_modules/.bin/_mocha"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "report-dir": "../../coverage_report/client-wfm",
    "produce-source-map": true,
    "check-coverage": false,
    "lines": 75,
    "functions": 100,
    "branches": 80
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/chai": "4.1.2",
    "@types/chai-as-promised": "7.1.0",
    "@types/mocha": "2.2.48",
    "@types/node": "9.4.6",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "del-cli": "1.1.0",
    "mocha": "5.0.1",
    "nyc": "11.4.1",
    "source-map-support": "0.5.3",
    "ts-node": "4.1.0",
    "typescript": "2.7.2"
  },
  "dependencies": {
    "@types/bluebird": "3.5.20",
    "@types/lodash": "4.14.104",
    "bluebird": "3.5.1",
    "lodash": "4.17.5"
  }
}
