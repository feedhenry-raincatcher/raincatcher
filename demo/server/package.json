{
  "name": "@raincatcher/demo-server",
  "version": "1.0.0",
  "description": "Raincatcher demo application server using express",
  "types": "src/index.ts",
  "main": "src/index.ts",
  "author": "feedhenry-raincatcher@redhat.com",
  "license": "Apache-2.0",
  "scripts": {
    "clean": "del coverage_report src/*.js src/**/*.js src/*.map src/**/*.map test/**/*.js test/**/*.map",
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "startDebug": "ts-node --inspect -debug-brk src/index.ts",
    "watch": "nodemon",
    "test": "nyc mocha"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "report-dir": "coverage_report",
    "check-coverage": true,
    "lines": 75,
    "functions": 60,
    "branches": 70
  },
  "devDependencies": {
    "@raincatcher/logger": "1.0.0",
    "@types/bluebird": "^3.5.5",
    "@types/body-parser": "^1.16.3",
    "@types/cookie-parser": "^1.3.30",
    "@types/cors": "^2.8.1",
    "@types/express": "^4.0.35",
    "@types/express-handlebars": "0.0.29",
    "@types/lodash": "^4.14.66",
    "@types/mocha": "^2.2.41",
    "@types/mongodb": "^2.2.7",
    "@types/morgan": "^1.7.32",
    "@types/node": "^8.0.7",
    "@types/serve-favicon": "^2.2.28",
    "del-cli": "^1.0.0",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "nyc": "^11.0.1",
    "source-map-support": "^0.4.15",
    "ts-node": "^3.0.4",
    "typescript": "^2.3.4"
  },
  "dependencies": {
    "@raincatcher/wfm-rest-api": "1.0.0",
    "@raincatcher/auth-passport": "1.0.0",
    "@raincatcher/datasync-cloud": "1.0.0",
    "@raincatcher/logger": "1.0.0",
    "@raincatcher/wfm-user": "1.0.0",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.3",
    "express": "^4.15.3",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.15.0",
    "keycloak-connect": "^3.2.0",
    "lodash": "^4.17.4",
    "mongodb": "^2.2.30",
    "morgan": "^1.8.1",
    "serve-favicon": "^2.4.3"
  }
}
