{
  "name": "@raincatcher/demo-server",
  "version": "1.0.0",
  "description": "Raincatcher demo application server using express",
  "types": "src/index.ts",
  "main": "src/index.ts",
  "author": "feedhenry-raincatcher@redhat.com",
  "license": "Apache-2.0",
  "scripts": {
    "clean": "del coverage_report src/*.js src/**/*.js src/*.map src/**/*.map test/**/*.js test/**/*.map",
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "watch": "nodemon",
    "test": "nyc mocha"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "report-dir": "coverage_report",
    "check-coverage": true,
    "lines": 75,
    "functions": 60,
    "branches": 70
  },
  "devDependencies": {
    "del-cli": "^1.1.0",
    "grunt": "^1.0.1",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-env": "^0.4.4",
    "grunt-exec": "^2.0.0",
    "mocha": "^3.5.0",
    "nodemon": "^1.11.0",
    "nyc": "^11.1.0",
    "source-map-support": "^0.4.16",
    "ts-node": "^3.3.0",
    "typescript": "^2.5.0"
  },
  "dependencies": {
    "@raincatcher/auth-passport": "1.0.0",
    "@raincatcher/datasync-cloud": "1.0.0",
    "@raincatcher/logger": "1.0.0",
    "@raincatcher/wfm-rest-api": "1.0.0",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "express": "^4.15.4",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.15.5",
    "keycloak-connect": "^3.2.1",
    "lodash": "^4.17.4",
    "mongodb": "^2.2.31",
    "morgan": "^1.8.2",
    "serve-favicon": "^2.4.3"
  }
}
