{
  "name": "@raincatcher/wfm-rest-api",
  "version": "1.1.1",
  "description": "Module used for building a RESTful api on top of the WFM solution",
  "types": "src/index.ts",
  "author": "feedhenry-raincatcher@redhat.com",
  "license": "MIT",
  "main": "src/",
  "scripts": {
    "clean": "del coverage_report src/*.js src/*.map src/**/*.js src/**/*.map test/**/*.js test/**/*.map",
    "build": "tsc",
    "test": "npm run clean && nyc mocha"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "report-dir": "coverage_report",
    "check-coverage": true,
    "lines": 75,
    "functions": 80,
    "branches": 70
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@raincatcher/logger": "1.1.1",
    "@raincatcher/wfm": "1.1.1",
    "@types/bluebird": "3.5.18",
    "@types/express": "4.0.39",
    "@types/lodash": "4.14.85",
    "bluebird": "3.5.1",
    "express": "4.16.2",
    "lodash": "4.17.4",
    "shortid": "2.2.8"
  },
  "devDependencies": {
    "@types/chai": "4.0.5",
    "@types/chai-as-promised": "7.1.0",
    "@types/mocha": "2.2.44",
    "@types/proxyquire": "1.3.28",
    "@types/shortid": "0.0.29",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "del-cli": "1.1.0",
    "mocha": "4.0.1",
    "nyc": "11.3.0",
    "source-map-support": "0.5.0",
    "ts-node": "3.3.0",
    "typescript": "2.6.1"
  }
}
