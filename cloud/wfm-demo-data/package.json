{
  "name": "@raincatcher/wfm-demo-data",
  "version": "1.1.2",
  "description": "Example seed data for the RainCatcher project",
  "types": "src/index.ts",
  "author": "feedhenry-raincatcher@redhat.com",
  "license": "MIT",
  "main": "src/",
  "scripts": {
    "clean": "del coverage_report src/**/*.js src/**/*.map test/**/*.js test/**/*.map",
    "build": "tsc"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "report-dir": "coverage_report",
    "check-coverage": true,
    "lines": 75,
    "functions": 100,
    "branches": 80
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@raincatcher/logger": "1.1.2",
    "@raincatcher/wfm": "1.1.2",
    "@types/bluebird": "3.5.20",
    "@types/mongodb": "3.0.17",
    "bluebird": "3.5.1"
  },
  "devDependencies": {
    "del-cli": "1.1.0",
    "ts-node": "6.0.2",
    "typescript": "2.8.3"
  },
  "peerDependencies": {
    "mongodb": "2.2.33"
  }
}
