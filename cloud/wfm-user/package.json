{
  "name": "@raincatcher/wfm-user",
  "version": "1.1.2",
  "description": "Module responsible for user management",
  "types": "src/index.ts",
  "author": "feedhenry-raincatcher@redhat.com",
  "license": "MIT",
  "main": "src/",
  "scripts": {
    "clean": "del coverage_report src/**/*.js src/**/*.map test/**/*.js test/**/*.map",
    "build": "tsc",
    "test": "npm run clean && nyc mocha"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "report-dir": "coverage_report",
    "check-coverage": true,
    "lines": 75,
    "functions": 55,
    "branches": 80
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/chai": "4.1.2",
    "@types/chai-as-promised": "7.1.0",
    "@types/mocha": "2.2.48",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "del-cli": "1.1.0",
    "mocha": "5.0.1",
    "nyc": "11.4.1",
    "source-map-support": "0.5.3",
    "ts-node": "4.1.0",
    "typescript": "2.7.2"
  },
  "dependencies": {
    "@raincatcher/logger": "1.1.2",
    "@raincatcher/wfm-rest-api": "1.1.2",
    "@types/bluebird": "3.5.20",
    "@types/express": "4.11.1",
    "bluebird": "3.5.1",
    "express": "4.16.2"
  }
}
