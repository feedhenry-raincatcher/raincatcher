{
  "name": "@raincatcher/datasync-cloud",
  "version": "1.1.0",
  "description": "Raincatcher DataSync cloud",
  "types": "src/index.ts",
  "main": "src/",
  "author": "feedhenry-raincatcher@redhat.com",
  "license": "MIT",
  "scripts": {
    "clean": "del src/**/*.js src/**/*.map test/**/*.js test/**/*.map",
    "build": "tsc",
    "test": "nyc mocha"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "report-dir": "../../coverage_report/cloud-datasync",
    "check-coverage": true,
    "lines": 75,
    "functions": 75,
    "branches": 50
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@raincatcher/logger": "1.1.0",
    "@types/body-parser": "^1.16.8",
    "@types/cors": "^2.8.3",
    "@types/express": "^4.0.39",
    "@types/mocha": "^2.2.44",
    "@types/mongodb": "^2.2.16",
    "@types/node": "^8.0.53",
    "@types/proxyquire": "^1.3.28",
    "del-cli": "^1.1.0",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "proxyquire": "^1.8.0",
    "source-map-support": "^0.5.0",
    "ts-node": "^3.3.0",
    "typescript": "^2.6.1"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "fh-sync": "^1.0.13"
  }
}
