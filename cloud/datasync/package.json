{
  "name": "@raincatcher/datasync-cloud",
  "version": "1.1.2",
  "description": "Raincatcher DataSync cloud",
  "types": "src/index.ts",
  "main": "src/",
  "author": "feedhenry-raincatcher@redhat.com",
  "license": "MIT",
  "scripts": {
    "clean": "del src/**/*.js src/**/*.map test/**/*.js test/**/*.map",
    "build": "tsc",
    "test": "nyc mocha"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "report-dir": "../../coverage_report/cloud-datasync",
    "check-coverage": true,
    "lines": 75,
    "functions": 75,
    "branches": 50
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@raincatcher/logger": "1.1.2",
    "@types/mocha": "5.2.0",
    "@types/proxyquire": "1.3.28",
    "del-cli": "1.1.0",
    "mocha": "5.1.1",
    "nyc": "11.7.1",
    "proxyquire": "2.0.1",
    "source-map-support": "0.5.5",
    "ts-node": "6.0.2",
    "typescript": "2.8.3"
  },
  "dependencies": {
    "@types/express": "4.11.1",
    "@types/mongodb": "3.0.17",
    "@types/node": "10.0.3",
    "express": "4.16.3",
    "fh-sync": "1.0.14"
  },
  "peerDependencies": {
    "mongodb": "2.2.33"
  }
}
