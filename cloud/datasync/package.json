{
  "name": "@raincatcher/datasync-cloud",
  "version": "1.0.0",
  "description": "Raincatcher DataSync cloud",
  "types": "src/index.ts",
  "main": "src/",
  "author": "feedhenry-raincatcher@redhat.com",
  "license": "Apache-2.0",
  "scripts": {
    "clean": "del src/**/*.js src/**/*.map test/**/*.js test/**/*.map",
    "build": "tsc",
    "test": "nyc mocha"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "report-dir": "../../coverage_report/cloud-datasync",
    "check-coverage": true,
    "lines": 75,
    "functions": 80,
    "branches": 50
  },
  "devDependencies": {
    "del-cli": "^1.1.0",
    "mocha": "^3.5.0",
    "nyc": "^11.1.0",
    "ts-node": "^3.3.0",
    "typescript": "^2.5.0"
  },
  "dependencies": {
    "@raincatcher/logger": "1.0.0",
    "body-parser": "^1.17.2",
    "cors": "^2.8.4",
    "express": "^4.15.4",
    "fh-sync": "^1.0.7"
  }
}
