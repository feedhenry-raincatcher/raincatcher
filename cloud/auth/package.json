{
  "name": "@raincatcher/express-auth",
  "version": "1.1.2",
  "description": "Authentication interface for express based applications",
  "types": "src/index.ts",
  "author": "feedhenry-raincatcher@redhat.com",
  "license": "MIT",
  "main": "src/",
  "scripts": {
    "clean": "del src/*.js src/**/*.js src/*.map src/**/*.map test/*.js test/**/*.js test/*.map test/**/*.map",
    "build": "tsc",
    "test": "npm run clean && nyc mocha",
    "example": "ts-node ./example/index.ts"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "report-dir": "../../coverage_report/cloud-passportauth",
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/mocha": "5.2.0",
    "del-cli": "1.1.0",
    "mocha": "5.1.1",
    "nyc": "11.7.1",
    "source-map-support": "0.5.5",
    "ts-node": "6.0.2",
    "typescript": "2.8.3"
  },
  "dependencies": {
    "@types/express": "4.11.1",
    "express": "4.16.3"
  },
  "peerDependencies": {
    "express-session": "1.15.6"
  }
}
