{
  "name": "@raincatcher/auth-passport",
  "version": "1.1.2",
  "description": "Passport authentication module",
  "types": "src/index.ts",
  "author": "feedhenry-raincatcher@redhat.com",
  "license": "MIT",
  "main": "src/",
  "scripts": {
    "clean": "del src/*.js src/**/*.js src/*.map src/**/*.map test/*.js test/**/*.js test/*.map test/**/*.map",
    "build": "tsc",
    "test": "npm run clean && nyc mocha",
    "example": "ts-node ./example/index.ts"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "report-dir": "../../coverage_report/cloud-passportauth",
    "check-coverage": true,
    "lines": 70,
    "functions": 70,
    "branches": 60
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@raincatcher/logger": "1.1.2",
    "@types/chai": "4.1.3",
    "@types/mocha": "5.2.0",
    "@types/sinon": "4.3.1",
    "del-cli": "1.1.0",
    "mocha": "5.1.1",
    "nyc": "11.7.1",
    "sinon": "5.0.3",
    "source-map-support": "0.5.5",
    "ts-node": "6.0.2",
    "typescript": "2.8.3"
  },
  "dependencies": {
    "@raincatcher/express-auth": "1.1.2",
    "@types/express": "4.11.1",
    "@types/jsonwebtoken": "7.2.7",
    "@types/passport": "0.4.5",
    "@types/passport-jwt": "3.0.1",
    "@types/passport-local": "1.0.33",
    "chai": "4.1.2",
    "express": "4.16.3",
    "express-session": "1.15.6",
    "jsonwebtoken": "8.2.1",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0"
  }
}
